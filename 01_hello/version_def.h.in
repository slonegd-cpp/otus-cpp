/**
 *  Этот файл преобразуется cmake-ом в обычные дефайны
 *  с подстановкой переменных cmake
 *  результат version_def.h
 */
#pragma once

#cmakedefine PROJECT_VERSION_MAJOR @PROJECT_VERSION_MAJOR@
#cmakedefine PROJECT_VERSION_MINOR @PROJECT_VERSION_MINOR@
#cmakedefine PROJECT_VERSION_PATCH @PROJECT_VERSION_PATCH@

#if !defined PROJECT_VERSION_MAJOR
    #define PROJECT_VERSION_MAJOR 0
#endif
#if !defined PROJECT_VERSION_MINOR
    #define PROJECT_VERSION_MINOR 0
#endif
#if !defined PROJECT_VERSION_PATCH
    #define PROJECT_VERSION_PATCH 0
#endif